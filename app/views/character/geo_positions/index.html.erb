<h1>Listing character_geo_positions</h1>

<table>
  <tr>
    <th>Character</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @character_geo_positions.each do |character_geo_position| %>
  <tr>
    <td><%= character_geo_position.character_id %></td>
    <td><%= character_geo_position.latitude %></td>
    <td><%= character_geo_position.longitude %></td>
    <td><%= link_to 'Show', character_geo_position %></td>
    <td><%= link_to 'Edit', edit_character_geo_position_path(character_geo_position) %></td>
    <td><%= link_to 'Destroy', character_geo_position, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

